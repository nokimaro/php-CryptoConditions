{
    "name": "kryuu-common/crypto-conditions",
    "description": "Base58 Encoding and Decoding Library for PHP",
    "repositories": [ 
            {
            "type": "vcs",
            "url": "https://github.com/nokimaro/php-Base58"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nokimaro/php-CryptoConditions"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nokimaro/php-Buffer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nokimaro/php-JsonStableStringify"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nokimaro/php-BigChainDB"
        }        
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "kryuu-common/buffer": "dev-master",
        "sop/asn1": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KryuuCommon\\CryptoConditions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KryuuCommon\\CryptoConditionsTest\\":   "test/"
        }
    }
}
